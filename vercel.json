{
    "vercel": 2,
    "builds": [
        {
            "src": "api/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/generate-prompt",
            "dest": "/api/index.js",
            "methods": ["POST"]
        },
        {
            "src": "/(.*)",
            "dest": "/public/$1"
        }
    ]
}